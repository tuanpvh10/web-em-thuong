@model WebEmThuong.Models.ShopViewModel
<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Menu</title>
    <link href="https://stackpath.bootstrapcdn.com/bootstrap/4.5.2/css/bootstrap.min.css" rel="stylesheet">
</head>
<body>
    <section id="hero" class="banner">
        <div class="middle-allign-wrapper">
            <div class="mask">
                <a href="/files/Bâu's Menu.pdf"><h1 class="page-title">Menu</h1></a>
            </div>
        </div>
    </section>
    <section class="top-padding-section">

        <div class="two-column-container product-container">
            <div class="product-sidebar">
                <div class="widget-wrapper">
                    <h4 class="product-sidebar-title">Categories</h4>
                    @foreach (var item in Model.Categories)
                    {
                        <div class="w-dyn-list">
                            <div role="list" class="vertical-wrapper _20px-gap w-dyn-items">
                                <div role="listitem" class="w-dyn-item"><a href="#" class="widget-link">@Html.DisplayFor(modeItem => item.Name)</a></div>
                            </div>
                        </div>
                    }
                </div>
                <div class="widget-wrapper">
                    <h4 class="product-sidebar-title">Rating</h4>
                    <div class="vertical-wrapper _30px-gap">
                        <div class="horizontal-between-wrapper">
                            <div class="rating-block sidebar-rating">
                                <div class="rating-icon"></div>
                                <div class="rating-icon"></div>
                                <div class="rating-icon"></div>
                                <div class="rating-icon"></div>
                                <div class="rating-icon"></div>
                            </div>
                            <div class="total-ratings">(10)</div>
                        </div>
                        <div class="horizontal-between-wrapper">
                            <div class="rating-block sidebar-rating">
                                <div class="rating-icon"></div>
                                <div class="rating-icon"></div>
                                <div class="rating-icon"></div>
                                <div class="rating-icon"></div>
                                <div class="rating-icon disabled"></div>
                            </div>
                            <div class="total-ratings">(0)</div>
                        </div>
                        <div class="horizontal-between-wrapper">
                            <div class="rating-block sidebar-rating">
                                <div class="rating-icon"></div>
                                <div class="rating-icon"></div>
                                <div class="rating-icon"></div>
                                <div class="rating-icon disabled"></div>
                                <div class="rating-icon disabled"></div>
                            </div>
                            <div class="total-ratings">(0)</div>
                        </div>
                        <div class="horizontal-between-wrapper">
                            <div class="rating-block sidebar-rating">
                                <div class="rating-icon"></div>
                                <div class="rating-icon"></div>
                                <div class="rating-icon disabled"></div>
                                <div class="rating-icon disabled"></div>
                                <div class="rating-icon disabled"></div>
                            </div>
                            <div class="total-ratings">(0)</div>
                        </div>
                        <div class="horizontal-between-wrapper">
                            <div class="rating-block sidebar-rating">
                                <div class="rating-icon"></div>
                                <div class="rating-icon disabled"></div>
                                <div class="rating-icon disabled"></div>
                                <div class="rating-icon disabled"></div>
                                <div class="rating-icon disabled"></div>
                            </div>
                            <div class="total-ratings">(0)</div>
                        </div>
                    </div>
                </div>
            </div>
            <div class="wrapper w-dyn-list">
                <div role="list" class="product-grid w-dyn-items">
                    @foreach (var item in Model.Productions)
                    {
                        <div role="listitem" class="w-dyn-item">
                            <div class="product-card-wrapper">
                                <a href="#" class="product-thumbnail-wrapper w-inline-block">
                                    <img alt="" loading="lazy" width="70" data-wf-sku-bindings="%5B%7B%22from%22%3A%22f_main_image_4dr%22%2C%22to%22%3A%22src%22%7D%5D" src="@item.ImgUrl" class="product-thumbnail" /></a>
                                <div class="product-body">
                                    <div class="rating-block">
                                        <div class="rating-icon"></div>
                                        <div class="rating-icon"></div>
                                        <div class="rating-icon"></div>
                                        <div class="rating-icon"></div>
                                        <div class="rating-icon"></div>
                                        <div class="total-ratings">(10)</div>
                                    </div>
                                    <a href="/product/chicken-alfredo-pasta" class="mask w-inline-block">
                                        <div class="sub-title">@Html.DisplayFor(modelItem => item.Name)</div>
                                    </a>
                                    <div class="snippet">
                                        <p>@Html.DisplayFor(modelItem => item.Description)</p>
                                    </div>
                                </div>
                                <div class="hover-buttons">
                                    <a href="/product/chicken-alfredo-pasta" class="product-icon-link w-inline-block">
                                        <div class="product-hover-icon"></div>
                                    </a>
                                    <a href="/product/chicken-alfredo-pasta" class="product-icon-link _16px-icon w-inline-block">
                                        <div></div>
                                    </a>
                                    <a href="/product/chicken-alfredo-pasta" class="product-icon-link w-inline-block">
                                        <div></div>
                                    </a>
                                </div>
                            </div>
                        </div>
                    }
                </div>
                <div role="navigation" aria-label="List" class="w-pagination-wrapper">
                    <a href="?9eb0920b_page=2" aria-label="Next Page" class="w-pagination-next pagination-button">
                        <div class="w-inline-block">Next</div>
                        <svg class="w-pagination-next-icon" height="12px" width="12px" xmlns="http://www.w3.org/2000/svg" viewBox="0 0 12 12" transform="translate(0, 1)"><path fill="none" stroke="currentColor" fill-rule="evenodd" d="M4 2l4 4-4 4"></path></svg>
                    </a>
                    <link rel="prerender" href="?9eb0920b_page=2" />
                </div>
            </div>
        </div>
    </section>
    <script src="https://d3e54v103j8qbb.cloudfront.net/js/jquery-3.5.1.min.dc5e7f18c8.js?site=659662e6ea1f446bb2be088b" type="text/javascript" integrity="sha256-9/aliU8dGd2tb6OSsuzixeV4y/faTqgFtohetphbbj0=" crossorigin="anonymous"></script>
    <script src="https://assets-global.website-files.com/659662e6ea1f446bb2be088b/js/webflow.214b53277.js" type="text/javascript"></script>
</body>
</html>
